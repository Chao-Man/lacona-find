{
  "name": "lacona-find",
  "version": "1.0.0",
  "description": "Lacona Addon to Open the parent directory of an application, file or folder in finder with it selected.",
  "main": "lib/index.js",
  "lacona": {
    "title": "lacona-find",
    "description": "Open the parent directory of an application, file or folder in finder with it selected.",
    "config": "config.json"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "lacona",
    "lacona-addon"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Chao-Man/lacona-find"
  },
  "dependencies": {
    "lacona-phrases": "^1.0.1",
    "lacona-api": "^1.0.1",
    "elliptical": "^1.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-cli": "^6.0.0",
    "rimraf": "^2.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  }
}
